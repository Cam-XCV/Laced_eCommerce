uestAll()\n    }, [])\n    \n    return(\n        <Router>\n            <>\n            <Switch>\n\n\n\n            <Route exact path=\"/trainers/:id\" render={(props) => {\n                const id = props.match.params.id\n                const trainer = findTrainerById(id)\n                return <TrainerDetail trainer={trainer} />\n            }} />\n\n            <Route exact path = \"/customer/:id\" render={(props) => {\n                const id = props.match.params.id\n                const customer = findCustomerById(id)\n                return <CustomerDetail customer={customer}  />\n            }} />\n\n            <Route exact path=\"/customer\" render={() => {\n                return <CustomerBox customers={customers} />\n            }} />\n\n            \n\n            <Route render={() => {\n                return <ShopBox trainers={trainers} />\n            }} />\n\n            \n\n           \n\n            </Switch>\n            </>\n        </Router>\n    )\n}\n\nexport default ShopContainer;","/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/components/NavBar.js",["61"],"import React from 'react';\n\nconst NavBar = () => {\n    return(\n        <header className=\"nav-container\">\n            <h1>Nav</h1>\n            <img className=\"header-logo\"/> \n                <ul>\n                    <li className=\"navLink\">\n                        <a href=\"/\">Home</a>\n                    </li>\n                    <li className=\"navLink\">\n                        <a href=\"/customer\">Sign In / Create Account</a>\n                    </li>\n                </ul>\n        </header>\n    )\n}\n\nexport default NavBar;","/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/components/ShopBox.js",[],"/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/components/trainers/TrainerDetail.js",["62"],"import React from 'react';\nimport { Link } from 'react-rout