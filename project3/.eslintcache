il';\nimport AdminTrainer from '../components/administrator/AdminTrainer';\n\n\nconst ShopContainer = () => {\n\n    const [trainers, setTrainers] = useState([]);\n    const [customers, setCustomers] = useState([]);\n    const [administrators, setAdministrator] = useState([]);\n\n    const requestAll = function(){\n        const request = new Request();\n        const trainerPromise = request.get('/trainers')\n        const customerPromise = request.get('/customers')\n        const administratorPromise = request.get('/administrators')\n\n        Promise.all([trainerPromise, customerPromise, administratorPromise])\n        .then((data) => {\n        setTrainers(data[0])\n        setCustomers(data[1])\n        setAdministrator(data[2])\n        })\n    }\n\n    const findTrainerById = function(id){\n        return trainers.find((trainer) => {\n            return trainer.id === parseInt(id)\n        })\n    }\n\n    const findCustomerById = function(id){\n        return customers.find((customer) => {\n            return customer.id === parseInt(id)\n        })\n    }\n\n    const customerLoggedIn = customers.filter((customer) => {\n        return customer.loggedIn === true;\n    })\n    \n\n    const handleCustomerPost = function(customer){\n        const request = new Request();\n        request.post(\"/customers\", customer)\n        .then(() => window.location = \"/customers/account\")\n    }\n\n    const handleTrainerPost = function(trainer){\n        const request = new Request();\n        request.post(\"/trainers\", trainer)\n        .then(() => {\n            window.location = \"/administrators/admin\"\n        })\n    }\n\n    const handleCustomerUpdate = function(customer){\n        const request = new Request();\n        request.patch(\"/customers\" + customer.id, customer)\n        .then(() => window.location = \"/customers/\" + customer.id)\n    }\n\n\n    useEffect(() => {\n        requestAll()\n    }, [])\n    \n    return(\n        <Router>\n            <>\n            <Switch>\n\n\n\n            <Route exact path=\"/trainers/:id\" render={(props) => {\n                const id = props.match.params.id\n                const trainer = findTrainerById(id)\n                return <TrainerDetail trainer={trainer} customer = {customerLoggedIn[0]} />\n            }} />\n\n\n            <Route exact path=\"/customers/new\" render={() => {\n                return <CustomerForm onCreate={handleCustomerPost} />\n            }} />\n\n                <Route exact path = \"/customers/account\" render= {() => {\n                return <CustomerDetail  customer = {customerLoggedIn[0]}\n                onUpdate={handleCustomerUpdate}/>\n                }}/>\n\n            <Route exact path = \"/customers/cart\" render= {() => {\n                return <CustomerCart  customer = {customerLoggedIn[0]}/>\n                }}/>\n\n            <Route exact path = \"/customers/:id\" render={(props) => {\n                const id = props.match.params.id\n                const customer = findCustomerById(id)\n                return <CustomerDetail\n                 customer={customer}\n                 onUpdate={handleCustomerUpdate} />\n            }} />\n\n\n\n            <Route exact path=\"/customers/:id/edit\" render={(props) => {\n                const id = props.match.params.id;\n                const customer = findCustomerById(id);\n                return <CustomerForm customer={customer} onUpdate={handleCustomerUpdate} />\n            }} />\n\n            <Route exact path=\"/customers\" render={() => {\n                return <CustomerBox customers={customers} />\n            }} />\n\n            <Route exact path =\"/administrators/new\" render ={() => {\n                return <AdminTrainer \n                    administrator = {administrators[0]}\n                    trainers = {trainers}\n                    \n                />\n            }} />\n            \n            <Route exact path = \"/administrators/admin\" render={() => {\n                const administrator = administrators[0]\n                return <AdminDetail\n                 administrator={administrator}\n                 trainers = {trainers}\n                 />\n            }} />\n            \n\n            <Route exact path = \"/administrators\" render={() => {\n                return <AdminLogin administrator={administrators[0]} />\n            }} />\n\n            \n            \n            \n\n            <Route render={() => {\n                return <ShopBox trainers={trainers} customer = {customerLoggedIn[0]}/>\n            }} />\n\n            \n           \n\n            </Switch>\n            </>\n        </Router>\n    )\n}\n\nexport default ShopContainer;","/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/components/NavBar.js",["97","98"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faShoppingCart, faUser, faSignInAlt } from \"@fortawesome/free-solid-svg-icons\";\n\nconst NavBar = () => {\n    const shoppingCart = <FontAwesomeIcon icon={faShoppingCart} />\n    const user = <FontAwesomeIcon icon={faUser} />\n    const administrator = <FontAwesomeIcon icon={faSignInAlt} />\n    return(\n        <header className=\"nav-container\">\n            <div className=\"logo-container\">\n            <a href=\"/\"><img src=\"https://png2.cleanpng.com/sh/dc599486ac1265438bd34bdee36ab0a6/L0KzQYm3UsAzN6VufZH0aYP2gLBuTgNvbZJwfeR8LYPrf7a0lvVkfJD3ReRAbn7sfri0kBhwbaQyTdMCNHTlSLOCVPJlOGgzUaoANEi3SIe4VcE4PmE8UKMENkC8QnB3jvc=/kisspng-sneakers-shoe-vector-running-shoes-5a74db8b94bd07.9854848615176078196092.png\" className=\"header-logo\"/></a>\n            </div>\n                <ul className=\"nav-links\">\n                    <li className=\"navLink\">\n                      <a href=\"/administrators\"><i>{administrator}</i></a>  \n                    </li>  \n                    <li className=\"navLink\">\n                        <a href=\"/customers\">Sign In / Create Account</a>\n                    </li>\n                    <li className=\"navLink\">\n                        <a href=\"/customers/cart\"><i>{shoppingCart}</i></a>\n                    </li>\n                    <li className=\"navLink\">\n                        <a href=\"/customers/account\"><i>{user}</i></a>\n                    </li>\n                </ul>\n        </header>\n    )\n}\n\nexport default NavBar;","/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/components/ShopBox.js",[],"/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/components/trainers/TrainerDetail.js",["99","100","101"],"import React, { useEffect, useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport Request from '../../helpers/request';\nimport './TrainerDetail.css'\nimport StarRatingComponent from 'react-star-rating-component';\nimport {Alert} from 'reactstrap';\n\nconst TrainerDetail = ({trainer, customer}) => {\n    const [card, setCard] = useState({})\n    const [sneaker, setSneaker] = useState({})\n    const [name, setName] = useState({})\n    const [alertOpen, setAlertOpen] = useState(false);\n\n    useEffect(() => {\n        if(trainer){\n            const currentCard = document.querySelector(\".card\")\n            setCard(currentCard)\n            const currentSneaker = document.querySelector(\".sneaker img\")\n            setSneaker(currentSneaker)\n            const currentName = document.querySelector(\".info h1\")\n            setName(currentName);\n        }\n        \n\n    }, [card])\n    \n\n    if(!trainer){\n        return <p>\n            Loading\n        </p>\n    }\n\n    \n\n\n    const trainerColours = trainer.colours.map((colour, index) => {\n        return(\n            <option key={index} value={index}>{colour}</option>\n        )\n    })\n\n    const onUpdate = function(customer){\n        const request = new Request();\n        const url = \"/customers/\" + customer.id;\n        request.patch(url, customer)\n        .then(() => console.log(\"Item Added to cart\"))\n    }\n\n    const addTrainerToCart = function(){\n        if(customer){\n            customer.cart.push(trainer);\n            setAlertOpen(true)\n            onUpdate(customer);\n        }\n        \n    }\n\n\n\n    const transform = function(e){\n        let xAxis = (window.innerWidth / 2 - e.pageX) / 20\n        let yAxis = (window.innerHeight / 2 - e.pageY) / 20\n\n        \n        if(card !== null){\n            card.style.transform = `rotateY(${xAxis}deg) rotateX(${yAxis}deg)`;\n\n        }\n        \n        \n    }\n\n    const animateOut = function(e){\n\n        if(card != null){\n            card.style.transition = 'all 0.5s ease';\n            card.style.transform = `rotateY(0deg) rotateX(0deg)`;\n            name.style.transform = \"translateZ(0px)\";\n            sneaker.style.transform = \"translateZ(0px) rotateZ(0deg)\"  \n             \n        }\n        \n        \n    }\n\n    const resetAlert = function(e){\n        setAlertOpen(false)\n    }\n\n    const animateIn = function(e){\n\n        if(card != null){\n            card.style.transition = 'none';\n            name.style.transform = \"translateZ(150px)\";\n            sneaker.style.transform = \"translateZ(250px)\";\n        }\n        \n        \n    }\n\n\n    return(\n        <>\n        <div className=\"page-action\">\n        <Link to =\"/\"><button className=\"back-button\">Back</button></Link>\n        <Alert isOpen={alertOpen} toggle={resetAlert} className=\"alert\">Item Added to Cart</Alert>\n        </div>\n        <div className=\"detail-body\">\n            <div className=\"container\" onMouseMove={transform} onMouseLeave={animateOut} onMouseEnter={animateIn}>\n                <div className=\"card\" >\n                    <div className=\"sneaker\">\n                        <div className=\"circle\"></div>\n                        <img src={trainer.image}/>\n                    </div>\n                    <div className=\"info\">\n                        <h1>{trainer.name}</h1>\n                        <h3>{trainer.brand}</h3>\n                        <p>£{trainer.price}</p>\n                        <p>Rating: <StarRatingComponent \n                        editing={false}\n                        isHalf={true}\n                        starCount={5}\n                        value={trainer.rating}\n                        /></p>\n                        {/* <button onClick={addTrainerToCart} className=\"purchase\">Add to cart</button> */}\n                    </div>\n                </div>\n            </div>\n            <button onClick={addTrainerToCart} className=\"purchase\">Add to cart</button>\n\n            \n        </div>\n        </>\n    )\n}\n\nexport default TrainerDetail;","/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/helpers/request.js",[],"/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/components/trainers/TrainerList.js",[],"/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/components/trainers/Trainer.js",["102"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport './Homepage.css'\n\nconst Trainer = ({trainer}) => {\n\n    if(!trainer){\n        return \"Loading...\"\n    }\n\n    const url = \"/trainers/\" + trainer.id\n\n    return(\n        <>\n        <div className=\"trainer\">\n        <Link to = {url}><img className=\"trainer-preview-image\" src={trainer.image} /></Link>\n            <p className=\"trainer-name\"> {trainer.name}</p>\n            <p className=\"trainer-price\"> £{trainer.price}</p>\n        </div>\n            \n        </>\n    )\n\n\n}\n\nexport default Trainer;","/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/components/CustomerBox.js",[],"/Users/cyrilishabiyi/codeclan_work/final_project/Project3_TrainerShop/project3/src/components/customers/CustomerDetail.js",["103","104"],"import React from 'react';\n\nimport { Link } from 'react-router-dom';\nimport Request from '../../helpers/request';\nimport CustomerBox from '../CustomerBox';\nimport CustomerOrder from './CustomerOrder';\nimport './order.css'\nimport './customer.css'\n\n\n\n    \n    \n\nconst CustomerDetail = ({customer, onUpdate}) => {\n\n    if(!customer){\n        return <p className=\"login-message\">\n            Login to View Your Details\n        </p>\n    }\n\n    const onChange = function(customer){\n        const request = new Request();\n        const url = \"/customers/\" + customer.id;\n        request.patch(url, customer)\n        .then(() => window.location = \"/customers\")\n    }\n\n\n    const logOut = function(){\n        customer.loggedIn = false;\n        onChange(customer)\n    }\n\n    const onBuy = function(customer){\n        const request = new Request();\n        const url = \"/customers/\" + customer.id;\n        request.patch(url, customer)\n        .then(() => window.location = \"/customers/cart\")\n    }\n\n    const addToCart = function(shoes){\n        for(let shoe of shoes){\n            customer.cart.push(shoe)\n        }\n        onBuy(customer);\n    }\n\n    \n\n\n\n    const orderNodes = customer.previousOrders.map((order, index) =>{\n        return(\n            <li key={index} className=\"order-details\" >\n\t        <div >\n\t            <CustomerOrder order={order} />\n                <button onClick={() => {addToCart(order.shoes)}}className=\"buy-again\">Buy Again</button>\n\t        </div>\n\t     </li>\n        )\n    })\n\n\n    \n\n\n\n    const editUrl = \"/customers/\" + customer.id + \"/edit\" \n    return(\n        <>\n        <div className=\"customer-container\">\n            <h1>{customer.name}</h1>\n            <img src={customer.avatar} className=\"customer-avatar\"></img>\n            <div classNam